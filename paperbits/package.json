{
    "name": "paperbits-demo-react",
    "version": "0.1.306",
    "description": "An example of using Paperbits to build web apps with rich content editing capability.",
    "author": "Paperbits",
    "license": "MIT",
    "keywords": [
        "paperbits",
        "demo"
    ],
    "scripts": {
        "start": "webpack-dev-server --host 0.0.0.0 --disableHostCheck true --open --config webpack.develop.js",
        "build": "webpack --config webpack.build.js",
        "build-designer": "webpack --config webpack.designer.js",
        "build-publisher": "webpack --config webpack.publisher.js",
        "build-runtime": "webpack --config webpack.runtime.js",
        "publish": "webpack --config webpack.publisher.js && node dist/publisher/index.js && npm run preview",
        "preview": "http-server ./dist/website -o -a localhost",
        "postinstall": "npm rebuild node-sass"
    },
    "devDependencies": {
        "@types/knockout.mapping": "^2.0.35",
        "@types/knockout.validation": "0.0.37",
        "@types/react": "^16.9.46",
        "autoprefixer": "9.8.6",
        "awesome-typescript-loader": "^5.2.1",
        "clean-webpack-plugin": "2.0.2",
        "copy-webpack-plugin": "^6.0.3",
        "css-loader": "4.2.1",
        "file-loader": "^6.0.0",
        "html-loader": "^1.2.1",
        "http-server": "^0.12.3",
        "mini-css-extract-plugin": "^0.10.0",
        "node-sass": "^4.14.1",
        "path": "^0.12.7",
        "postcss-loader": "^3.0.0",
        "raw-loader": "^4.0.1",
        "sass-loader": "9.0.3",
        "style-loader": "1.2.1",
        "terser": "5.2.1",
        "terser-webpack-plugin": "4.1.0",
        "typescript": "4.0.2",
        "url-loader": "4.1.0",
        "webpack": "4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "3.11.0",
        "webpack-merge": "^5.1.2"
    },
    "dependencies": {
        "@paperbits/common": "0.1.323",
        "@paperbits/core": "0.1.323",
        "@paperbits/emails": "0.1.323",
        "@paperbits/firebase": "0.1.323",
        "@paperbits/forms": "0.1.323",
        "@paperbits/gtm": "0.1.323",
        "@paperbits/intercom": "0.1.323",
        "@paperbits/prosemirror": "0.1.323",
        "@paperbits/styles": "0.1.323",
        "@webcomponents/custom-elements": "^1.4.2",
        "core-js": "^3.6.5",
        "knockout": "^3.5.1",
        "knockout-mapping": "^2.6.0",
        "knockout.validation": "^2.0.4",
        "mime-types": "^2.1.27",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    }
}
